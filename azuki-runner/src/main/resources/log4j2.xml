<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>

    <Appenders>

        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %p (%t) [%c{1}] %m%n" />
        </Console>

    </Appenders>

    <Loggers>

        <Logger name="com.anaplan.engineering.azuki" level="info" />

        <Root level="error">
            <AppenderRef ref="CONSOLE"/>
        </Root>

    </Loggers>

</Configuration>
